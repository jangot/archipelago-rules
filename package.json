{
  "name": "zng",
  "version": "0.0.1",
  "description": "Zirtue Next Generation Server Platform",
  "author": "Michael LeDuc",
  "private": true,
  "license": "UNLICENSED",
  "types": "dist/index.d.ts",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "build": "nest build",
    "build:all": "./scripts/build-all.sh",
    "build:all-win": "powershell -ExecutionPolicy Bypass -File ./scripts/build-all.ps1",
    "clean": "node -e \"require('fs').rmSync('./dist', { recursive: true, force: true });\"",
    "clean:modules": "rimraf node_modules package-lock.json",
    "generate:queries": "pgtyped --config ./pgtyped.config.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/core/main",
    "start:all": "concurrently \"npm run start core\" \"npm run start notification\" \"npm run start payment\"",
    "start:non-core": "concurrently \"npm run start notification\" \"npm run start payment\"",
    "start:non-notif": "concurrently \"npm run start core\" \"npm run start payment\"",
    "start:non-payment": "concurrently \"npm run start core\" \"npm run start notification\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:errors": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/core/test/jest-e2e.json",
    "migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create apps/core/src/data/repository/migrations/migration",
    "migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d apps/core/src/data/repository/postgresql/config.ts  apps/core/src/data/repository/migrations/migration",
    "migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d apps/core/src/data/repository/postgresql/config.ts",
    "migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d apps/core/src/data/repository/postgresql/config.ts",
    "find:unused": "npx ts-prune"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.848.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@pgtyped/runtime": "^2.4.2",
    "@ssut/nestjs-sqs": "^3.0.1",
    "@types/pluralize": "^0.0.33",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "camelcase-keys": "^9.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crc-32": "^1.2.2",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "nestjs-graceful-shutdown": "^1.0.3",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.2",
    "phone": "^3.1.66",
    "pino-pretty": "^13.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@pgtyped/cli": "^2.4.3",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.1.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "glob": "^11.0.2",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "pg-mem": "^3.0.4",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
